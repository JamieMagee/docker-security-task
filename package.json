    "@types/node": "10.17.60",
    "@types/string.prototype.matchall": "4.0.0",
    "@vercel/ncc": "0.32.0",
    "cpy-cli": "3.1.1",
    "jest": "27.4.0",
    "npm-run-all": "4.1.5",
